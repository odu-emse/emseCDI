{
    "name": "emseCDI",
    "version": "0.0.4",
    "description": "Asynchronous Offline Degree Platform",
    "build": {
        "appId": "com.example.electron-boilerplate",
        "directories": {
            "output": "dist",
            "buildResources": "public"
        },
        "afterSign": "scripts/notarize.js",
        "mac": {
            "category": "public.app-category.developer-tools",
            "extendInfo": {
                "NSUserNotificationAlertStyle": "alert"
            },
            "hardenedRuntime": true,
            "gatekeeperAssess": false,
            "entitlements": ".config/entitlements.mac.plist",
            "entitlementsInherit": ".config/entitlements.mac.plist",
            "publish": [
                {
                    "provider": "generic",
                    "url": "https://files.twstyled.com/releases/${name}/${os}/"
                }
            ]
        },
        "dmg": {
            "background": "public/dmgBackground.png",
            "icon": "icon.icns",
            "iconSize": 155,
            "window": {
                "width": 660,
                "height": 400
            },
            "contents": [
                {
                    "x": 123,
                    "y": 172
                },
                {
                    "x": 539,
                    "y": 168,
                    "type": "link",
                    "path": "/Applications"
                }
            ]
        },
        "win": {
            "publisherName": "twstyled contributors",
            "sign": null,
            "publish": [
                {
                    "provider": "generic",
                    "url": "https://files.twstyled.com/releases/${name}/${os}/"
                }
            ]
        },
        "asar": false,
        "extraMetadata": {
            "main": "main/index.js"
        },
        "files": [
            {
                "filter": [
                    "package.json",
                    "tailwind.config.js"
                ]
            },
            {
                "from": "./dist",
                "to": "./",
                "filter": [
                    "index.html"
                ]
            },
            {
                "from": "./dist/main",
                "to": "./main"
            },
            {
                "from": "./dist/assets",
                "to": "./assets"
            }
        ]
    },
    "scripts": {
        "clean": "rimraf yarn.lock && rimraf ./**/.next && rimraf dist && rimraf ./**/out && rimraf {**/,}node_modules",
        "dev": "cross-env NODE_ENV=development node -r esbuild-register ./scripts/dev.ts",
        "preview": "cross-env NODE_ENV=production electron ./dist/main",
        "dev:renderer": "vite",
        "dev:main": "cross-env NODE_ENV=development node -r esbuild-register ./scripts/dev.ts",
        "build": "rimraf dist && vite build && npm run prod:esbuild",
        "prod:esbuild": "cross-env NODE_ENV=production node -r esbuild-register ./scripts/prod.ts",
        "prod:tsc": "tsc --project ./src/main/tsconfig.json",
        "publish": "concurrently \"npm:publish:mac\" \"npm:publish:win\"",
        "publish:mac": "electron-builder --macos",
        "publish:win": "env-cmd -r ./.config/.env-secrets.json -e production electron-builder --win"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/odu-emse/emseCDI.git"
    },
    "keywords": [],
    "author": "DÃ¡niel B. Papp",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/odu-emse/emseCDI/issues"
    },
    "homepage": "https://github.com/odu-emse/emseCDI#readme",
    "devDependencies": {
        "@sync-labs/eslint-config": "2.2.6",
        "@types/file-saver": "^2.0.3",
        "@types/node": "^16.10.2",
        "@types/react": "^17.0.34",
        "@types/react-dom": "^17.0.11",
        "@types/react-router-dom": "^5.3.1",
        "@typescript-eslint/eslint-plugin": "^5.3.0",
        "@typescript-eslint/parser": "^5.3.0",
        "@vitejs/plugin-react": "^1.0.7",
        "autoprefixer": "^10.4.0",
        "chalk": "^4.1.2",
        "concurrently": "^6.3.0",
        "cross-env": "^7.0.3",
        "electron": "^15.1.0",
        "electron-builder": "^22.10.5",
        "electron-notarize": "^1.1.1",
        "env-cmd": "^10.1.0",
        "esbuild": "^0.13.13",
        "esbuild-register": "^3.1.2",
        "eslint": "8.1.0",
        "eslint-config-airbnb": "18.2.1",
        "eslint-config-prettier": "8.3.0",
        "eslint-plugin-import": "^2.24.2",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-prettier": "4.0.0",
        "eslint-plugin-react": "^7.26.1",
        "generate-changelog": "^1.8.0",
        "postcss": "^8.3.8",
        "prettier": "^2.4.1",
        "rimraf": "^3.0.2",
        "tailwindcss": "^2.2.19",
        "typescript": "^4.4.3",
        "vite-plugin-twstyled": "^3.2.4"
    },
    "dependencies": {
        "@tailwindcss/aspect-ratio": "^0.3.0",
        "@uiw/react-md-editor": "^3.6.5",
        "@vitejs/plugin-react-refresh": "^1.3.6",
        "electron-context-menu": "^3.1.1",
        "electron-is-dev": "^2.0.0",
        "electron-reload": "^2.0.0-alpha.1",
        "electron-window-state": "^5.0.3",
        "fast-glob": "^3.2.7",
        "file-saver": "^2.0.5",
        "markdown-to-jsx": "^7.1.3",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-player": "^2.9.0",
        "react-router-dom": "5.1.2",
        "vite": "^2.6.13"
    }
}
