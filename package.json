{
    "name": "emseCDI",
    "version": "0.0.4",
    "description": "Asynchronous Offline Degree Platform",
    "main": "main/index.js",
    "build": {
        "appId": "com.example.electron-boilerplate",
        "files": [
            "node_modules/**/*",
            "package.json",
            "main",
            "src/out"
        ],
        "directories": {
            "buildResources": "assets/resources"
        }
    },
    "scripts": {
        "dev": "concurrently \"yarn dev:vite\" \" yarn dev:electron\"",
        "dev:vite": "vite",
        "dev:electron": "npm run build:electron && electron .",
        "build": "npm run build:vite && npm run build:electron",
        "build:vite": "vite build",
        "build:electron": "tsc -p electron",
        "dist": "npm run build && electron-builder",
        "pack": "npm run build && electron-builder --dir",
        "clean": "rimraf dist main src/out"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/odu-emse/emseCDI.git"
    },
    "keywords": [],
    "author": "DÃ¡niel B. Papp",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/odu-emse/emseCDI/issues"
    },
    "homepage": "https://github.com/odu-emse/emseCDI#readme",
    "devDependencies": {
        "@types/file-saver": "^2.0.3",
        "@types/node": "^16.10.2",
        "@types/react": "^17.0.34",
        "@types/react-dom": "^17.0.11",
        "@types/react-router-dom": "^5.3.1",
        "@typescript-eslint/eslint-plugin": "^5.3.0",
        "@typescript-eslint/parser": "^5.3.0",
        "@vitejs/plugin-react": "^1.0.7",
        "autoprefixer": "^10.4.0",
        "concurrently": "^6.3.0",
        "electron": "^15.1.0",
        "electron-builder": "^22.10.5",
        "eslint": "8.1.0",
        "eslint-config-airbnb": "18.2.1",
        "eslint-config-prettier": "8.3.0",
        "eslint-plugin-import": "^2.24.2",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-prettier": "4.0.0",
        "eslint-plugin-react": "^7.26.1",
        "generate-changelog": "^1.8.0",
        "postcss": "^8.3.8",
        "prettier": "^2.4.1",
        "tailwindcss": "^2.2.19",
        "typescript": "^4.4.3"
    },
    "dependencies": {
        "@tailwindcss/aspect-ratio": "^0.3.0",
        "@uiw/react-md-editor": "^3.6.5",
        "@vitejs/plugin-react-refresh": "^1.3.6",
        "electron-is-dev": "^2.0.0",
        "electron-reload": "^2.0.0-alpha.1",
        "fast-glob": "^3.2.7",
        "file-saver": "^2.0.5",
        "markdown-to-jsx": "^7.1.3",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-player": "^2.9.0",
        "react-router-dom": "5.1.2",
        "vite": "^2.6.13"
    }
}
